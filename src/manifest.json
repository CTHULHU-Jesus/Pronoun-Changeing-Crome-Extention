{
  "name": "Pronoun Changer",
  "action": {
    "default_popup": "popup.html"
    },
  "manifest_version": 3,
  "version": "0.1.0",
  "description": "Changes all pronouns used on a site to some specified ones.",

  "background": {
    "service_worker" : "background.js"
  },
  "permissions": [ "activeTab"
                 , "scripting" ]
}
