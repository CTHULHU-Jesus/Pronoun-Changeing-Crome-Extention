{
  "name": "Pronoun Changer",
  "action": {
    "default_popup": "popup.html"
    },
  "manifest_version": 3,
  "version": "0.1.0",
  "description": "Changes all pronouns used on a site to some specified ones.",
  "icons": {
    "16": "./imgs/16x16.png",
    "32": "./imgs/32x32.png",
    "48": "./imgs/48x48.png",
    "128": "./imgs/128x128.png"
  },
  "background": {
    "service_worker" : "background.js"
  },
  "permissions": [ "activeTab"
                 , "scripting" ]
}
